sole.log(res.data[0].Organization.Categories)\r\n              // res.data[0].Organization.Categories.forEach( el => {\r\n              //   this.categories[el.id] = [el.id, el.name]\r\n              // })\r\n              // this.tasks = res.data\r\n            })\r\n            .catch((error) => {\r\n              // handle error\r\n              console.log(error);\r\n            })\r\n            .then(_=> {\r\n              // always executed\r\n            });\r\n        }\r\n    },\r\n    created: function () {\r\n        if (localStorage.getItem('access_token')) {\r\n            this.showPage = 'pg-homepage'\r\n        } else {\r\n            this.showPage = 'pg-login'\r\n        }\r\n    }\r\n}\r\n</script>\r\n\r\n<style>\r\n\r\n</style>"},"lineCount":null}},"error":null,"hash":"2b18f1db93169b8141c574bcec4ace85","cacheData":{"env":{}}}