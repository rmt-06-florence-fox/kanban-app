 margin-bottom: 10px; /* Added */\r\n  }\r\n}\r\n\r\n// ============ NAVBAR =================\r\n\r\n.custom-navbar {\r\n  display:grid;\r\n  grid-template-columns: 2fr 1fr 2fr;\r\n\r\n  .left-navbar {\r\n    display: grid;\r\n    grid-gap: 5px;\r\n    grid-template-columns: repeat(2, auto);\r\n    justify-self: flex-start;\r\n    margin-left: 20px;\r\n\r\n    .nav-item {\r\n      height: 50px;\r\n      color: rgba(255, 255, 255, 0.7);\r\n      border-radius: 5px;\r\n      padding: 5px;\r\n    }\r\n  }\r\n\r\n  .center-navbar {\r\n    justify-self: center;\r\n    justify-content: center;\r\n\r\n    .logo {\r\n      font-size: 2rem;\r\n      font-weight: 700;\r\n      color: rgba(255, 255, 255, 0.7);\r\n      margin: 0 0.5rem;\r\n      transition: color 150ms;\r\n      cursor: pointer;\r\n      font-family: 'Yellowtail', cursive;\r\n\r\n      h2 {\r\n        margin-top: 0.3rem;\r\n        margin-bottom: 0.3rem;\r\n        text-align: center;\r\n      }\r\n    }\r\n\r\n    .logo-icon {\r\n      padding-right: 0.1rem;\r\n    }\r\n  }\r\n\r\n  .right-navbar {\r\n    display: grid;\r\n    grid-gap: 10px;\r\n    grid-template-columns: repeat(3, auto);\r\n    justify-self: flex-end;\r\n    margin-right: 20px;\r\n\r\n    .nav-item {\r\n      color: rgba(255, 255, 255, 0.7);\r\n      border-radius: 5px;\r\n      padding: 5px;\r\n    }\r\n  }\r\n}\r\n\r\n// ============= MAIN PAGE ================\r\n\r\n.container {\r\n  font-family: 'Source Sans Pro', sans-serif;\r\n}\r\n\r\n.task-lists {\r\n  display: grid;\r\n  grid-auto-columns: 260px;\r\n  grid-auto-flow: column;\r\n  grid-gap: 25px;\r\n  overflow-x: auto;\r\n}\r\n\r\n.task-lists::-webkit-scrollbar {\r\n  height: 1.6rem;\r\n}\r\n\r\n.task-lists::-webkit-scrollbar-thumb {\r\n  margin: 5px;\r\n  background-color: #6a89cc;\r\n  border: 0.3rem solid #4a69bd;\r\n}\r\n\r\n.task-list {\r\n  background: #dfe6e9;\r\n  display: grid;\r\n  grid-auto-rows: max-content;\r\n  grid-gap: 10px;\r\n  padding: 5px;\r\n  overflow-y: auto;\r\n  max-height: calc(100vh - 11rem);\r\n\r\n  .card :hover {\r\n    background-color: #eaeaea;\r\n  }\r\n}\r\n\r\n.task-list::-webkit-scrollbar {\r\n  height: 1.6rem;\r\n}\r\n\r\n.task-list::-webkit-scrollbar-thumb {\r\n  margin: 5px;\r\n  background-color: #5f5f5f;\r\n  border: 0.2rem solid #dfe6e9;\r\n}\r\n\r\n.container h4 {\r\n  font-size: 1.5rem;\r\n  font-weight: 700;\r\n  color: #333;\r\n  padding: 1rem;\r\n}\r\n\r\n.add-card-btn {\r\n  display: block;\r\n  font-size: 1.4rem;\r\n  font-weight: 400;\r\n  color: #838c91;\r\n  padding: 1rem;\r\n  text-align: left;\r\n  cursor: pointer;\r\n}\r\n\r\n.add-list-btn {\r\n  flex: 0 0 27rem;\r\n  display: block;\r\n  font-size: 1.4rem;\r\n  font-weight: 400;\r\n  background-color: #6a89cc;\r\n  color: #809ddb;\r\n  padding: 1rem;\r\n  border-radius: 0.3rem;\r\n  cursor: pointer;\r\n  transition: background-color 150ms;\r\n  text-align: left;\r\n}\r\n\r\n.edit-icon :hover {\r\n  text-decoration: none;\r\n  background-color: #121212;\r\n}\r\n\r\n\r\n// FOOTER\r\n\r\n.footer {\r\n  background-color: #82ccdd;\r\n  .container {\r\n    width: auto;\r\n    max-width: 680px;\r\n    padding: 0 15px;\r\n  }\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"75d2a9f203706a4de6cfd512e3496d75","cacheData":{"env":{}}}